	<include name="WallInfoAnimation">
		<animation type="Visible" reversible="false">
			<effect type="fade" start="0" end="100" time="400" delay="3085" />
			<effect type="zoom" center="auto" start="0" end="100" time="550" delay="3000" tween="back" acceleration="-2" />
		</animation>
	</include>

	<include name="WallInfoOpenAnimationAuto">
		<animation type="WindowOpen" reversible="false">
			<effect type="fade" start="0" end="100" time="400" delay="3085" />
			<effect type="zoom" center="auto" start="0" end="100" time="550" delay="3000" tween="back" acceleration="-2" />
		</animation>
	</include>

	<include name="WallInfoOpenAnimationOn">
		<animation effect="fade" start="0" end="100" time="400">WindowOpen</animation>
	</include>

